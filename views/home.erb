<section id="add">
  <form name="my_form" action="/" method="post">
    <input id="auto-location" autofocus name="location" placeholder="Your location&hellip;" ></input>
    <br><br><br>
    <input id="auto-winner1" name="winner1" placeholder="Winner first player&hellip;"></input>
    <br><br><br>
    <input id="auto-winner2" name="winner2" placeholder="Winner second player&hellip;"></input>
    <br><br><br>
    <input id="auto-loser1" name="loser1" placeholder="Loser first player&hellip;"></input>
    <br><br><br>
    <input id="auto-loser2" name="loser2" placeholder="Loser second player&hellip;"></input>
    <br><br><br>
    <input type="submit" value="Submit!">
  
  </form>

    <script>
      function loadJSON(callback) {   
        console.log("loadjson started");

        var xobj = new XMLHttpRequest();
        xobj.overrideMimeType("application/json");
                  xobj.open('GET', 'locations', true);
                  xobj.onreadystatechange = function () {
                    if (xobj.readyState == 4 && xobj.status == "200") {
                                                callback(xobj.responseText);
                                              }
                                            };
                                            xobj.send(null);  
                                          }
                                          function init() {
                                           loadJSON(function(response) {
                                             console.log(response);
       // Parse JSON string into object
       var actual_JSON = JSON.parse(response);
       console.log("hey man");

       console.log(actual_JSON);
     });
                                         }

                                         init();
    </script>




    <script src="/js/auto-complete.js"></script> 
</section>

<% @games.each do |game| %>
  <article>
    <p>
    <%= game.winner1 %> and <%= game.winner2 %> beat <%= game.loser1 %> and 
      <%= game.loser2 %> on <%= game.created_at.strftime("%m/%d/%y") %> in  <%= game.location %>
      <span><a href="/<%= game.id %>">[edit]</a></span>
    </p>
  </article>
<% end %>

